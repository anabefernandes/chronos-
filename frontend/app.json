{
  "expo": {
    "name": "Chronos",
    "slug": "chronos",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "chronos",
    "icon": "./assets/expoGo/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/expoGo/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#3C188F"
    },
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/expoGo/logo.png",
        "backgroundColor": "#3C188F"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expoGo/logo.png",
        "backgroundColor": "#3C188F"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.anonymous.chronos"
    },
    "web": {
      "favicon": "./assets/expoGo/logo_daweb.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-audio"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e8b4e91e-976d-4b09-9733-5a23ae65d463"
      }
    }
  }
}
